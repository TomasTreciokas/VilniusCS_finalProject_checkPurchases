<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.text.Text?>
<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="actionsPackage.admin.AdminLoginController">
    <children>
        <Text fx:id="keypassText" layoutX="15.0" layoutY="60.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Admin's keyPass" />
        <TextField fx:id="keypass" layoutX="145.0" layoutY="43.0" prefHeight="17.0" prefWidth="130.0" />
        <Button fx:id="submitBtn" layoutX="180.0" layoutY="90.0" mnemonicParsing="false" onAction="#check_Password" text="Submit" />
    </children>
</AnchorPane>

